%div{:class => "ui-body"}
  %img{:align => "left", :src => me.photo.largePhotoUrl + "?oauth_token=#{client.oauth_token}", :style => "margin-right: 12px"}
  %strong #{me.name}
  %br
  #{me.username}
  %br
  #{client.org_id}
  %br
  #{client.instance_url} 
%div{:class=>"content-primary"}
  = htmlize_hash("You",me)
  %div{:"data-role" => "collapsible", :"data-content-theme"=>"c"}
    %h3 Objects (#{sobjects.size})
    %p
      %ul{:"data-role" => "listview"}
        - sobjects.each do |sobject_name|
          %li
            %a{:href => "/describe/#{sobject_name}"} #{sobject_name}
